# OpenAI API Key (required for OpenAI integration)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (alternative to OpenAI)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Tailscale Auth Key (required for Tailscale integration)
TS_AUTHKEY=your_tailscale_auth_key_here

# Database URL (optional, defaults to SQLite)
# DATABASE_URL=sqlite:////app/data/openmemory.db

# Server Configuration
HOST=0.0.0.0
PORT=8765

# Default User/App (optional)
DEFAULT_USER_ID=default_user
DEFAULT_CLIENT_NAME=default_app

# Qdrant Configuration (optional, uses Docker networking by default)
# QDRANT_HOST=localhost
# QDRANT_PORT=6333